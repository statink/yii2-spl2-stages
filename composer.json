{
    "name": "statink/yii2-spl2-stages",
    "description": "stage images asset bundle for stat.ink",
    "license": "MIT",
    "type": "yii2-extension",
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@bouhime.com"
        }
    ],
    "require": {
        "php": ">= 8.2",
        "yiisoft/yii2": "^2.0.50"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.42"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "statink\\yii2\\stages\\spl2\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "yiisoft/yii2-composer": true
        },
        "fxp-asset": {
            "enabled": false
        },
        "process-timeout": 1800
    }
}
